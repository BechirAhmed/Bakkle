aws elb create-load-balancer-policy --load-balancer-name TESTCLUSTER --policy-name TESTCLUSTER-proxy-protocol --policy-type-name ProxyProtocolPolicyType --policy-attributes AttributeName=ProxyProtocol,AttributeValue=True

aws elb set-load-balancer-policies-for-backend-server --load-balancer-name TESTCLUSTER --instance-port 80 --policy-names TESTCLUSTER-proxy-protocol
aws elb set-load-balancer-policies-for-backend-server --load-balancer-name TESTCLUSTER --instance-port 81 --policy-names TESTCLUSTER-proxy-protocol